    "# 继承的语法\n",
    "# 在python中，任何类都有一个公共的父类——object\n",
    "\n",
    "class Person():\n",
    "    name = \"No one\"\n",
    "    age = 0\n",
    "    def sleep(self):\n",
    "        print(\"Sleeping……\")\n",
    " \n",
    "# 父类写在类后括号中      \n",
    "class Teacher(Person):\n",
    "    pass\n",
    "\n",
    "t = Teacher()\n",
    "print(t.name)\n",
    "print(Teacher.name)"
   ]
